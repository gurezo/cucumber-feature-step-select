    "cuid": "^1.3.8",
    "moment": "^2.19.2",
    "package-lock": "^1.0.0",
    "rxjs": "^5.5.6",
    "typeface-heebo": "0.0.22",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "1.7.3",
    "@angular/compiler-cli": "5.2.8",
    "@types/csv-parse": "^1.1.11",
    "@types/jasmine": "2.5.53",
    "@types/node": "^8.0.34",
