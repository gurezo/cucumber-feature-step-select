{
  "name": "cucumber-feature-step-select",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:local",
    "start:local": "ng serve",
    "start:external": "ng serve",
    "build": "ng build --prod && npm run post-build",
    "test": "ng test -sr",
    "test:multi": "ng test",
    "lint:js": "ng lint --type-check true",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "lint": "npm run lint:styles && npm run lint:js",
    "i18n": "ng xi18n --i18n-format xlf2 --output-path src/i18n --out-file messages.xlf --locale en && xliffmerge --profile xliffmerge.conf.json",
    "jenkins:test": "npm run lint && npm run test && npm run build",
    "post-build": "npm run post-build:version",
    "post-build:version": "node ./src/build/version.js",
    "start:sc": "ng serve",
    "scenario": "ng e2e -c protractor.scenario.conf.js",
    "scenario:ie": "ng e2e -c protractor.scenario.ie.conf.js"
  },
  "private": true,
  "dependencies": {
